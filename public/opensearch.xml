<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/" xmlns:moz="http://www.mozilla.org/2006/browser/search/">
    <ShortName>JMP</ShortName>
    <Description>JMP - Quick, shared navigation to websites and bookmarks</Description>
    <InputEncoding>UTF-8</InputEncoding>
    <Image width="128" height="128" type="image/png">https://jmp.castive.dev/favicon.png</Image>
